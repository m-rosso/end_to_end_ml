{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowWriteObjectsS3Bucket",
            "Effect": "Allow",
            "Action": "s3:PutObject",
            "Resource": "arn:aws:s3:::ml-app-inputs-preds/*"
        },
        {
            "Sid": "AllowSeeBucketListInTheConsole",
            "Effect": "Allow",
            "Action": [
                "s3:ListAllMyBuckets",
                "s3:GetBucketLocation"
            ],
            "Resource": "arn:aws:s3:::*"
        }
    ]
}